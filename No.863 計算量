#include <bits/stdc++.h>
#define PI 3.14159265359
#define NIL -1
using namespace std;
const int64_t MOD = 1e9 + 7;

int main() {
	int64_t A, B;
	cin >> A >> B;

	int64_t N1, N2;
	int64_t n, m;
	n = 200000;
	m = 5000;
	N1 = n / m;
	N2 = (n * n) / (m * m);
	//cout << N1 << endl;
	//cout << N2 << endl;

	if ((B / A) < N2) {
		cout << 1 << endl;
	}
	else {
		cout << 2 << endl;
	}
}
